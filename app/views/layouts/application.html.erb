<!DOCTYPE html>
<html>
<head>
  <title>Bbm</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </a>
        <a class="brand" href="#">Menu</a>
        <div class="nav-collapse">
          <ul class="nav">
            <li><%= link_to "Budget", budgets_path %></li>
            <li><%= link_to "Payment", payments_path %></li>
            <li><%= link_to "Account ", accounts_path %></li>
            <li><%= link_to "Manager Account ", manager_accounts_path %></li>
            <li><%= link_to "Competence Center", competence_centers_path %></li>
            <li><%= link_to "Supplier", suppliers_path %></li>
            <li><%= link_to "Recurrency", recurrencies_path %></li>
            <li><%= link_to "Notification", notifications_path %></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
<div class="container">
    <% flash.each do |name, msg| %>
  	<div class="alert alert-<%= name == :notice ? "success" : "error" %>">
    	<a class="close" data-dismiss="alert">Ã—</a>
    	<%= msg if msg%>
  	</div>
	<% end %>
  	<div>
     <%= breadcrumbs("=>") %>
    </div>
    <div class="row">
      <div class="span9"><%= yield %></div>
    </div>
  </div>
  
</body>
</html>
